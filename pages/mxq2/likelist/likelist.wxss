/* pages/mxq1/likelist/likelist.wxss */
page{
    background: #fff;
}
.container{
    width: calc(100vw - 60rpx);
    padding: 0 30rpx;
    min-height: 100vh;
    height: auto;
    background-color: #fff;
}
.list_row{
    padding: 30rpx 0 20rpx 0;
    height: 58rpx;
    width:100%;
    font-size: 30rpx;
    line-height: 58rpx;
    color:#333;
}
.avatar{
    width: 58rpx;
    height: 58rpx;
    margin-right: 30rpx;
}
.avatar image{
    width: 100%;
    height: 100%;
    border-radius: 50%;
}
.time{
    font-size: 26rpx;
    color:#707070; 
}